function arithmetic(a, b, operator){
  return (operator == "add") ? a+b : (operator == "subtract") ? a-b : (operator == "multiply") ? 
    a*b : (operator == "divide") ? a/b : 0
}

через switch case :\

function arithmetic(a, b, operator){
switch (operator) {
case "add" :
return a+b;
case "subtract":
return a-b;
case "multiply":
return a*b;
case "divide":
return a/b;
default : return 0

}
